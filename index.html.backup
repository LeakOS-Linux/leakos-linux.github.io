<script>
document.addEventListener('DOMContentLoaded', () => {
    const slides = document.querySelectorAll('.slide');
    const dotsContainer = document.getElementById('dots');
    let current = 0;
    const total = slides.length;
    let interval;

    // Buat dots kotak secara otomatis
    slides.forEach((_, i) => {
        const dot = document.createElement('span');
        dot.classList.add('carousel-dot');
        if (i === 0) dot.classList.add('active');
        dot.addEventListener('click', () => {
            current = i;
            showSlide();
            resetInterval();
        });
        dotsContainer.appendChild(dot);
    });

    const dots = document.querySelectorAll('.carousel-dot');

    function showSlide() {
        slides.forEach(s => s.classList.remove('active'));
        dots.forEach(d => d.classList.remove('active'));

        slides[current].classList.add('active');
        dots[current].classList.add('active');
    }

    function nextSlide() {
        current = (current + 1) % total;
        showSlide();
    }

    function startAuto() {
        interval = setInterval(nextSlide, 5000); // ganti 5000 = 5 detik
    }

    function resetInterval() {
        clearInterval(interval);
        startAuto();
    }

    // Mulai autoplay
    startAuto();

    // Pause saat hover (opsional, biar user bisa baca)
    document.querySelector('.carousel').addEventListener('mouseenter', () => clearInterval(interval));
    document.querySelector('.carousel').addEventListener('mouseleave', startAuto);
});
</script>

<script>
document.addEventListener('DOMContentLoaded', () => {
  const slidesContainer = document.getElementById('leakSlides');
  const dotsContainer   = document.getElementById('leakDots');
  const slides          = document.querySelectorAll('.leak-slide');
  const totalSlides     = slides.length;
  let current           = 0;
  let interval;

  // Buat dots secara otomatis
  slides.forEach((_, index) => {
    const dot = document.createElement('button');
    dot.classList.add('leak-dot');
    if (index === 0) dot.classList.add('active');
    
    dot.addEventListener('click', () => {
      current = index;
      updateSlider();
      resetAutoPlay();
    });
    
    dotsContainer.appendChild(dot);
  });

  const dots = document.querySelectorAll('.leak-dot');

  function updateSlider() {
    slidesContainer.style.transform = `translateX(-${current * 100}%)`;
    
   // Membuat event listener untuk setiap dot
   dots.forEach((dot, i) => {
   dot.addEventListener('click', () => {
   current = i;  // Ubah index 'current' berdasarkan dot yang diklik
   showSlide();   // Perbarui gambar yang ditampilkan
   resetInterval();  // Reset interval otomatis agar gambar tidak bergerak otomatis lagi
   });
   

  function nextSlide() {
    current = (current + 1) % totalSlides;
    updateSlider();
  }

  function startAutoPlay() {
    interval = setInterval(nextSlide, 4500); // 4.5 detik per slide
  }

  function resetAutoPlay() {
    clearInterval(interval);
    startAutoPlay();
  }

  // Pause on hover
  const carousel = document.querySelector('.leak-carousel');
  carousel.addEventListener('mouseenter', () => clearInterval(interval));
  carousel.addEventListener('mouseleave', startAutoPlay);

  // Mulai
  startAutoPlay();
});


</script>



<script>
document.addEventListener('DOMContentLoaded', () => {
    const slider = document.querySelector('.slider');
    const track = document.getElementById('logoTrack');
    const slides = document.querySelectorAll('.slidec');

    if (!slider || !track || slides.length === 0) return;

    const uniqueCount = 3;
    const totalSlides = slides.length;
    let isPaused = false;
    let isDragging = false;
    let startX = 0;
    let currentTranslate = 0;
    let animationID;

    // =============================================
    // Snap akurat ke index (mobile & desktop)
    // =============================================
    function snapToIndex(index) {
        if (index < 0 || index >= totalSlides) return;

        // Ambil ukuran real-time (penting untuk mobile)
        const slideRect = slides[0].getBoundingClientRect();
        const slideWidth = slideRect.width;
        const gap = parseFloat(getComputedStyle(track).gap) || 16;
        const effectiveSlideWidth = slideWidth + gap;

        const sliderWidth = slider.getBoundingClientRect().width;
        const centerOffset = (sliderWidth - slideWidth) / 2;

        // Posisi target slide
        const targetPosition = index * effectiveSlideWidth;

        // Translate agar tepat center
        let translateX = -(targetPosition - centerOffset);

        // Batasi translate agar tidak terlalu jauh (hindari glitch infinite)
        const maxTranslate = -( (totalSlides - 3) * effectiveSlideWidth ); // batas aman
        translateX = Math.max(translateX, maxTranslate);
        translateX = Math.min(translateX, 200); // jangan terlalu ke kanan

        // Apply snap
        track.style.transition = 'transform 0.45s ease-out';
        track.style.transform = `translateX(${translateX}px)`;

        currentTranslate = translateX;

        // Update active setelah transisi
        setTimeout(() => {
            slides.forEach(s => s.classList.remove('active'));

            // Aktifkan slide target & duplikatnya
            if (slides[index]) slides[index].classList.add('active');
            const dup = (index + uniqueCount) % totalSlides;
            if (slides[dup]) slides[dup].classList.add('active');

            track.style.transition = '';
        }, 450);
    }

    // =============================================
    // Klik gambar → snap & pause
    // =============================================
    slides.forEach((slide, index) => {
        slide.addEventListener('click', e => {
            if (isDragging) return;
            e.stopPropagation();

            isPaused = true;
            track.classList.add('paused');
            track.style.animationPlayState = 'paused';

            // Snap ke gambar yang diklik
            snapToIndex(index);
        });

        // Touch khusus mobile (double-tap optional, tapi tap sekali cukup)
        slide.addEventListener('touchend', e => {
            if (e.touches.length === 0 && !isDragging) {
                // Sama seperti click
                isPaused = true;
                track.classList.add('paused');
                track.style.animationPlayState = 'paused';
                snapToIndex(index);
            }
        }, { passive: true });
    });

    // =============================================
    // Drag sederhana (kurangi sensitivitas di mobile)
    // =============================================
    function startDrag(x) {
        startX = x;
        isDragging = false;
        track.style.animationPlayState = 'paused';
    }

    function moveDrag(x) {
        const diff = x - startX;
        if (Math.abs(diff) > 12) isDragging = true;
        // Drag ringan saja di mobile
        currentTranslate += diff * 0.6; // kurangi kecepatan drag
        track.style.transform = `translateX(${currentTranslate}px)`;
        startX = x; // update untuk smooth
    }

    function endDrag() {
        if (isDragging) {
            // Snap kembali ke posisi terdekat setelah drag
            const approxIndex = Math.round(-currentTranslate / (slides[0].offsetWidth + 16));
            snapToIndex(approxIndex % uniqueCount);
        }
        isDragging = false;
    }

    // Mouse
    slider.addEventListener('mousedown', e => { startDrag(e.pageX); e.preventDefault(); });
    slider.addEventListener('mousemove', e => { if (startX !== 0) moveDrag(e.pageX); });
    slider.addEventListener('mouseup', endDrag);
    slider.addEventListener('mouseleave', endDrag);

    // Touch mobile
    slider.addEventListener('touchstart', e => {
        startDrag(e.touches[0].pageX);
    }, { passive: false });

    slider.addEventListener('touchmove', e => {
        moveDrag(e.touches[0].pageX);
    }, { passive: false });

    slider.addEventListener('touchend', endDrag);

    // Resume jika tap di luar
    document.addEventListener('touchend', e => {
        if (isPaused && !slider.contains(e.target)) {
            resumeSlider();
        }
    });

    function resumeSlider() {
        isPaused = false;
        track.classList.remove('paused');
        track.style.animationPlayState = 'running';
        slides.forEach(s => s.classList.remove('active'));
        currentTranslate = 0;
        track.style.transform = 'translateX(0)';
    }

    console.log("Mobile-optimized slider loaded. Coba tap gambar di HP-mu.");
});
</script>

